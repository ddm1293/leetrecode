{
  "manifest_version": 3,
  "name": "LeetReCode",
  "version": "1.0.0",
  "description": "The chrome extension to support LeetReCode",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions":[
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
